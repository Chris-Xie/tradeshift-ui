{
	"verbose": true,
	"increment": "prerelease",
	"prereleaseId": "beta",
	"requireCleanWorkingDir": true,
	"changelogCommand": "echo '### Commits' && git log --pretty=format:'* %h - %s%n```%n%b%n```' [REV_RANGE] | cat; echo ",
	"src": {
		"commitMessage": "Release v%s",
		"tagName": "v%s",
		"tagAnnotation": "Tradeshift UI v%s",
		"beforeStageCommand": "grunt release-deploy"
	},
	"github": {
		"release": true,
		"releaseName": "v%s",
		"preRelease": true,
		"tokenRef": "GH_ACCESS_TOK"
	}
}
